    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "prosv5 make"
        },
        {
            "label": "build and deploy (slot 3)",
            "type": "shell",
            "command": "prosv5 mut --slot=3"
        },
        {
            "label": "attach to terminal",
            "type": "shell",
            "command": "prosv5 terminal"
        }
    ]
}